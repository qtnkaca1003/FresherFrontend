    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@use-gesture/react": "^10.2.10",
    "axios": "^0.26.1",
    "dayjs": "^1.11.0",
    "framer-motion": "^6.2.8",
    "i18next": "^21.6.14",
    "pakagelogin-nhanqt": "^0.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.17.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.0",
    "react-spinners": "^0.11.0",
    "sass": "^1.49.9",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:stub": "env-cmd -f .env.stub yarn start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:dev": "env-cmd -f .env.development yarn build",
    "build:stub": "env-cmd -f .env.stub yarn build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit && eslint '**/*.{ts,tsx}' --quiet --fix",
    "format": "prettier --write '**/*.{scss,json}'",
    "postinstall": "npx husky install",
    "sb": "start-storybook -p 6006 -s public",
    "build:sb": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "i18next-xhr-backend": "^3.2.2",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "react-test-renderer": "^17.0.2",
    "webpack": "^5.70.0"
  }
}
